{
	"extends": "tslint:recommended",
	"rules": {
        "max-line-length": {
            "options": [120]
        },
        "semicolon": true,
        "quotemark": [true, "single"],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": false,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        }
    }
}
